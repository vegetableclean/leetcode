class Solution:
    def isPalindrome(self, x: int) -> bool:
        thres=pow(2,31)
        pos_thres=thres-1
        neg_thres=-thres
        #print(thres)
        #print(pos_thres)
        #print(neg_thres)
        a=list(str(x))
        b=a[::-1]
        print(a)
        print(b)
        if a==b:
            return True
        else:
            return False
        
